{
  "name": "anthaathicommercemobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "jetify",
    "extract": "formatjs extract \"src/**/*.{ts,tsx,vue}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file src/locales/en-US.json",
    "compile": "formatjs compile-folder --ast --format simple src/locales src/compiled-locales",
    "i18n": "yarn extract && yarn compile"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-intl": "^6.0.3",
    "react-native": "0.68.2",
    "react-native-paper": "^4.12.1",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-vector-icons": "^9.1.0"
  },
  "workspaces": {
    "nohoist": [
      "react-native",
      "react-native/**",
      "react-native-dev-menu",
      "react-native-svg",
      "jetifier",
      "metro",
      "react",
      "react-native-safe-area-context",
      "react-native-safe-area-view",
      "react-native-paper",
      "react-native-vector-icons"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@formatjs/cli": "^5.0.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-formatjs": "^10.3.23",
    "eslint": "^7.32.0",
    "get-yarn-workspaces": "^1.0.2",
    "jest": "^26.6.3",
    "metro-config": "^0.56.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}

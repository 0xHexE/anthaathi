{
  "name": "anthaathi-commerce-web-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm --project tsconfig.server.json src-server/server",
    "start": "NODE_ENV=production node dist/server.js",
    "build": "yarn build:client && yarn build:server && yarn build:entry",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server  --outDir dist/server",
    "build:entry": "tsc --project tsconfig.server.json"
  },
  "dependencies": {
    "express": "^4.18.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@types/compression": "^1.7.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}

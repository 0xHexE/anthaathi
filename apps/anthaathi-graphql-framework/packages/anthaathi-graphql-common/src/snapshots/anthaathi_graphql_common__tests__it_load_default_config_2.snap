    port:
      - name: http
        number: 80
    http:
      - name: goingToFile
        match:
          - name: matchtogoogle
            uri:
              exact: /_api/graphql
              prefix: ~
              regex: ~
            method: ~
            headers: ~
            query_params: ~
            ignoreUriCase: ~
            withoutHeaders: ~
        header: ~
        destination:
          name: FileLoader
          config:
            fileMapping:
              - name: something
                file: data/$1.graphql


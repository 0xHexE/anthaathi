---
source: packages/anthaathi-graphql-common/src/lib.rs
expression: config
---
apiVersion: alphaV1
handler:
  - hosts:
      - $1.anthaathi.org
    http:
      - name: goingToFile
        match:
          - name: matchtogoogle
            uri:
              exact: /_api/graphql
              prefix: ~
              regex: ~
            headers: ~
            query_params: ~
            ignoreUriCase: ~
            withoutHeaders: ~
        header: ~
        destination:
          name: FileLoader
          config:
            fileMapping:
              - name: something
                file: data/$1.graphql
    listener:
      - http
listener:
  - name: http
    address: "localhost:3000"
    tls: ~

